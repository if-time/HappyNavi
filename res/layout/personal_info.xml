	       android:layout_marginRight="10dp"
	       android:background="@color/grey"
	       
	       />
	 </com.trackersurvey.helper.MyLinearLayout> -->
	 
	 <com.trackersurvey.helper.MyLinearLayout
	    android:id="@+id/Linearlayout_changepwd"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    
	    android:layout_below="@id/Linearlayout_myinfo"
	    android:orientation="vertical"
	    android:background="@color/listitem_normal"
	   >
	   <TextView 
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_margin="10dp"
	       android:text="@string/item_changepwd"
	       android:textSize="15sp"
	       android:textColor="#000000"
	       />
	   <View 
	       android:id="@+id/divider_03"
	       android:layout_width="match_parent"
	       android:layout_height="1dp"
	       android:layout_marginLeft="10dp"
	       android:layout_marginRight="10dp"
	       android:background="@color/grey"
	       
	       />
	 </com.trackersurvey.helper.MyLinearLayout>
	 <com.trackersurvey.helper.MyLinearLayout
	    android:id="@+id/Linearlayout_sportdata"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    
	    android:layout_below="@id/Linearlayout_changepwd"
	    android:orientation="vertical"
	    android:background="@color/listitem_normal"
	   >
	   <TextView 
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_margin="10dp"
	       android:text="@string/item_sportdata"
	       android:textSize="15sp"
	       android:textColor="#000000"
	       />
	   
	   <View 
	       android:id="@+id/divider_04"
	       android:layout_width="match_parent"
	       android:layout_height="1dp"
	       android:layout_marginLeft="10dp"
	       android:layout_marginRight="10dp"
	       android:background="@color/grey"
	       
	       />
	   
	 </com.trackersurvey.helper.MyLinearLayout>
	 
	 <com.trackersurvey.helper.MyLinearLayout
	    android:id="@+id/Linearlayout_logout"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    
	    android:layout_below="@id/Linearlayout_sportdata"
	    android:orientation="vertical"
	    android:layout_marginTop="30dp"
	    android:gravity="center_vertical"
	    android:background="@color/red"
	   >
	   <TextView 
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_margin="10dp"
	       android:text="@string/logout"
	       android:textSize="15sp"
	       android:textColor="#ffffff"
	       />
	   
	 </com.trackersurvey.helper.MyLinearLayout>

</RelativeLayout>
